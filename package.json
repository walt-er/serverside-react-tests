{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "authors": [
    "Walter Mitchell <walt@walt.codes>"
  ],
  "license": "ISC",
  "homepage": "https://github.com/walt-er/serverside-react-tests",
  "repository": {
    "type": "git",
    "url": "https://github.com/walt-er/serverside-react-tests.git"
  },
  "bugs": {
    "url": "https://github.com/walt-er/serverside-react-tests/issues"
  },
  "main": "app/index.js",
  "scripts": {
    "start": "node ./build/server.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/server.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "stylelint": "stylelint ./src/styles/main.scss",
    "eslint:dev": "eslint --ext .js --ext .jsx ./src/scripts/* ./src/components/*",
    "eslint:prod": "eslint -c ./.eslintrc-prod --ext .js --ext .jsx ./src/scripts/* ./src/components/*",
    "vet:dev": "npm run stylelint && npm run eslint:dev",
    "vet:prod": "npm run stylelint && npm run eslint:prod",
    "stop": "sudo killall -9 node",
    "tkill": "taskkill /f /im node.exe"
  },
  "pre-commit": [
    "vet:dev"
  ],
  "author": "",
  "dependencies": {
    "autoprefixer": "8.4.1",
    "axios": "^0.18.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression": "^1.7.0",
    "copy-webpack-plugin": "4.5.1",
    "css-hot-loader": "1.3.9",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "express": "^4.15.4",
    "express-http-proxy": "^1.0.6",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "lodash": "^4.17.4",
    "node-sass": "4.9.0",
    "node-sass-json-importer": "3.2.0",
    "nodemon": "^1.12.0",
    "npm-run-all": "^4.1.1",
    "path": "0.12.7",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "2.1.5",
    "postcss-reporter": "5.0.0",
    "pre-commit": "1.2.2",
    "prop-types": "^15.6.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "7.0.1",
    "serialize-javascript": "^1.4.0",
    "style-loader": "0.21.0",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-webpack-plugin": "0.10.4",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.7.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
